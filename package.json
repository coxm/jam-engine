{
  "name": "jam-engine",
  "version": "17.0.1",
  "description": "Game jam engine",
  "repository": {
    "type": "git",
    "url": "github:coxm/jam-engine"
  },
  "main": "build/index.js",
  "scripts": {
    "start": "NODE_ENV=development node ./devserver.js",
    "build": "NODE_ENV=production webpack --config webpack.config.js",
    "test": "NODE_ENV=testing jest"
  },
  "keywords": [
    "game",
    "jam",
    "engine"
  ],
  "author": "coxm",
  "license": "GPL-3.0",
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/p2": "^0.7.31",
    "@types/pixi.js": "^4.7.3",
    "awesome-typescript-loader": "^5.0.0",
    "jasmine-core": "2.5.2",
    "jest": "^22.4.3",
    "ts-jest": "^22.4.4",
    "typescript": "^2.4.1",
    "webpack": "^4.6.0",
    "webpack-dev-server": "^3.1.3"
  },
  "dependencies": {
    "p2": "^0.7.1",
    "pixi.js": "4.3.4"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/test/.*\\.ts)",
    "moduleNameMapper": {
      "jam/(.*)": "<rootDir>/src/$1.ts"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
